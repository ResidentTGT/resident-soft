		"@secretlint/secretlint-rule-secp256k1-privatekey": "^10.2.2",
		"@types/copy-paste": "^2.1.0",
		"@types/crypto-js": "^4.2.2",
		"@types/imap-simple": "^4.2.10",
		"@types/module-alias": "^2.0.4",
