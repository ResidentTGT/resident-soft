		"puppeteer-extra-plugin-block-resources": "^2.4.3",
		"puppeteer-extra-plugin-recaptcha": "^3.6.8",
		"react": "^19.1.1",
		"react-dom": "^19.1.1",
		"rebrowser-puppeteer-core": "^24.8.1",
		"seedrandom": "^3.0.5",
		"socks-proxy-agent": "^8.0.5",
		"starknet": "^7.6.4",
		"uuid": "^11.1.0"
	},
	"devDependencies": {
		"@eslint/eslintrc": "^3.3.1",
		"@eslint/js": "^9.32.0",
