		"grammy": "^1.37.0",
		"https-proxy-agent": "^7.0.6",
		"imap-simple": "^5.1.0",
		"jose": "^6.0.12",
		"jsonc-parser": "^3.3.1",
		"lorem-ipsum": "^2.0.8",
		"node-localstorage": "^3.0.5",
		"puppeteer": "^24.15.0",
		"puppeteer-core": "^24.15.0",
		"puppeteer-extra": "^3.3.6",
		"puppeteer-extra-plugin-block-resources": "^2.4.3",
