{
	"name": "Resident Soft",
	"version": "0.1.10",
	"description": "",
	"main": "index.ts",
	"type": "commonjs",
	"workspaces": [
		"frontend"
	],
	"scripts": {
		"tsc": "tsc",
		"start:frontend": "npm run dev -w frontend",
		"build:frontend": "npm run build -w frontend",
		"start:backend": "npm run bundle:backend && node ./dist/index.js",
		"restart:backend": "node ./build/index.js",
		"bundle:backend": "node ./scripts/bundle.mjs",
		"create_sea_blob": "node --experimental-sea-config ./configs/sea-config.json",
		"write_sea_windows": "npx postject resident_app.exe NODE_SEA_BLOB sea-prep.blob --sentinel-fuse NODE_SEA_FUSE_fce680ab2cc467b6e072b8b5df1996b2 --overwrite",
		"write_sea_linux": "npx postject resident_app NODE_SEA_BLOB sea-prep.blob --sentinel-fuse NODE_SEA_FUSE_fce680ab2cc467b6e072b8b5df1996b2 --overwrite",
		"write_sea_mac": "npx postject resident_app NODE_SEA_BLOB sea-prep.blob --sentinel-fuse NODE_SEA_FUSE_fce680ab2cc467b6e072b8b5df1996b2 --overwrite --macho-segment-name NODE_SEA ",
		"sea_build": "npm run bundle:backend && npm run create_sea_blob  && node scripts/touch_app_exe.js &&  npm run write_sea_windows && npm run write_sea_linux && npm run write_sea_mac",
		"depcruise": "depcruise ./src --config configs/dependency-cruiser/.dependency-cruiser.js  --output-type dot  > configs/dependency-cruiser/dependency-graph.dot",
		"lint": "eslint . --config ./configs/eslint.config.mjs",
		"lint-fix": "eslint . --fix --config ./configs/eslint.config.mjs",
		"prettier": "prettier --config ./configs/.prettierrc --check  \"**/*.+(js|ts)\"",
		"prettier-fix": "prettier --config ./configs/.prettierrc --write \"**/*.+(js|ts)\"",
		"encrypt_premium": "node ./scripts/decryption.js encryption=true filePath=src/premium.zip folderPath=src/premium",
		"decrypt_premium": "node ./scripts/decryption.js encryption=false filePath=src/premium.zip folderPath=src/premium",
		"secretlint": "npx secretlint --secretlintrc ./configs/.secretlintrc.json --secretlintignore ./configs/.secretlintignore \"**/*\"",
		"typecheck": "tsc -p tsconfig.json --noEmit"
	},
	"author": "",
	"license": "ISC",
	"dependencies": {
		"@1inch/fusion-sdk": "^2.3.6",
		"@2captcha/captcha-solver": "^1.3.0",
		"@aptos-labs/ts-sdk": "^4.0.0",
		"@mysten/sui": "^1.37.1",
		"@solana-program/compute-budget": "^0.6.1",
		"@solana-program/system": "^0.6.2",
		"@solana-program/token": "^0.4.1",
		"@solana/web3.js": "^2.0.0-rc.4",
		"axios": "^1.11.0",
		"bip39": "^3.1.0",
		"bs58": "^6.0.0",
		"copy-paste": "^2.2.0",
		"crypto-js": "^4.2.0",
		"ethers": "^6.15.0",
		"exceljs": "^4.4.0",
		"express": "^5.1.0",
		"ghost-cursor": "^1.4.1",
		"grammy": "^1.37.0",
		"https-proxy-agent": "^7.0.6",
		"imap-simple": "^5.1.0",
		"jose": "^6.0.12",
